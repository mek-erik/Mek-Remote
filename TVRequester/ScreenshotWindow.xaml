<Window x:Class="TVRequester.ScreenshotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TVRequester"
        mc:Ignorable="d"
        Title="ScreenshotWindow" Height="768" Width="1366 ">
    <Grid>
        <Image x:Name="imgScreenshot" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Fill" ></Image>
    </Grid>
</Window>
